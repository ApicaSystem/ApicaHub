            "type": "metrics",
            "queriesList": [
                {
                    "chart_type": "counter",
                    "data_source_name": "Apica Monitoring",
                    "name": "Messages / sec",
                    "options": {
                        "description": "Messages / sec",
                        "label": "events / sec",
                        "order": 1,
                        "plot": {
                            "errorColumn": "",
                            "groupBy": "",
                            "x": "Timestamp",
                            "xLabel": "Timestamp",
                            "y": [
                                "value"
                            ],
                            "yLabel": "value"
                        },
                        "upperLimit": ""
                    },
                    "query": "round(sum(rate(logiq_message_count[5m])),1)",
                    "schema": "logiq_message_count"
                },
                {
                    "chart_type": "counter",
                    "data_source_name": "Apica Monitoring",
                    "name": "Log events (24h)",
                    "options": {
                        "label": "million",
                        "description": "Log events (24h)",
                        "order": 2,
                        "plot": {
                            "errorColumn": "",
                            "groupBy": "",
                            "x": "Timestamp",
                            "xLabel": "Timestamp",
                            "y": [
                                "value"
                            ],
                            "yLabel": "value"
                        },
                        "upperLimit": ""
                    },
                    "query": "round(sum(increase(logiq_message_count[24h]))/1000000,0.001)",
                    "schema": "logiq_message_count"
                },
                {
                    "chart_type": "counter",
                    "data_source_name": "Apica Monitoring",
                    "name": "Total ingested bytes (30d)",
                    "options": {
                        "label": "GB",
                        "description": "Total ingested bytes (30d)",
                        "order": 3,
                        "plot": {
                            "errorColumn": "",
                            "groupBy": "",
                            "x": "Timestamp",
                            "xLabel": "Timestamp",
                            "y": [
                                "value"
                            ],
                            "yLabel": "value"
                        },
                        "upperLimit": ""
                    },
                    "query": "round(sum(increase(logiq_data_received_bytes[30d]))/1000000000,0.01)",
                    "schema": "logiq_data_received_bytes"
                },
                {
                    "chart_type": "gauge",
                    "data_source_name": "Apica Monitoring",
                    "name": "Ingest capacity utilization",
                    "options": {
                        "description": "Ingest capacity utilization",
